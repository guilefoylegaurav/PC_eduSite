<h1> Add Student </h1>
<form method = "POST" action="/superadmin/students/add">
  <label for="fname">Username:</label><br>
  <input type="text" id="fname" name="username"><br>
  <label for="lname">Password</label><br>
  <input type="password" id="lname" name="password"><br><br>
  <label for="lname">Name:</label><br>
  <input type="text" id="lname" name="name"><br><br>
  <label for="lname">Email:</label><br>
  <input type="email" id="lname" name="email"><br><br>
  <label for="lname">Contact Number:</label><br>
  <input type="text" id="lname" name="contact"><br><br>
  <label for="lname">Father's Name:</label><br>
  <input type="text" id="lname" name="father"><br><br>
  <label for="lname">Mother's Name:</label><br>
  <input type="text" id="lname" name="mother"><br><br>
  <label for="lname">Address:</label><br>
  <input type="text" id="lname" name="address"><br><br>
  <label for="lname">College/School:</label><br>
  <input type="text" id="lname" name="college"><br><br>
  <label for="lname">Class:</label><br>
  <input type="text" id="lname" name="class"><br><br>
  <label for="lname">Batch:</label><br>
  <input type="text" id="lname" name="batch"><br><br>

  <label for="lname">Select subject</label><br>

  <%subject.forEach((subject1)=>{%>     
    <input type="checkbox" id="lname" name="subject[]" value="<%=subject1._id%>"><%=subject1.subject_id%>(<%=subject1.name%>)<br><br>
  <%})%>

  <input type="submit" value="Submit">
</form> 
